        <jsScript_script>&#x2f;&#x2f; Where the work gets done&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f; JSON Object that will eventually be emitted&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f;var example &#x3d;&#xd;&#xa;&#x2f;&#x2f;&#x5b;&#x7b;&#xd;&#xa;&#x2f;&#x2f;		&#x22;toBottom&#x22; &#x3a; true,&#xd;&#xa;&#x2f;&#x2f;		&#x22;cells&#x22; &#x3a; &#x5b;&#x7b;&#xd;&#xa;&#x2f;&#x2f;				&#x22;columnId&#x22; &#x3a; 7960873114331012,&#xd;&#xa;&#x2f;&#x2f;				&#x22;value&#x22; &#x3a; true&#xd;&#xa;&#x2f;&#x2f;			&#x7d;, &#x7b;&#xd;&#xa;&#x2f;&#x2f;				&#x22;columnId&#x22; &#x3a; 642523719853956,&#xd;&#xa;&#x2f;&#x2f;				&#x22;value&#x22; &#x3a; &#x22;New status&#x22;,&#xd;&#xa;&#x2f;&#x2f;				&#x22;strict&#x22; &#x3a; false&#xd;&#xa;&#x2f;&#x2f;			&#x7d;&#xd;&#xa;&#x2f;&#x2f;		&#x5d;&#xd;&#xa;&#x2f;&#x2f;	&#x7d;, &#x7b;&#xd;&#xa;&#x2f;&#x2f;		&#x22;toBottom&#x22; &#x3a; true,&#xd;&#xa;&#x2f;&#x2f;		&#x22;cells&#x22; &#x3a; &#x5b;&#x7b;&#xd;&#xa;&#x2f;&#x2f;				&#x22;columnId&#x22; &#x3a; 7960873114331012,&#xd;&#xa;&#x2f;&#x2f;				&#x22;value&#x22; &#x3a; true&#xd;&#xa;&#x2f;&#x2f;			&#x7d;, &#x7b;&#xd;&#xa;&#x2f;&#x2f;&#x2f;				&#x22;columnId&#x22; &#x3a; 642523719853956,&#xd;&#xa;&#x2f;&#x2f;				&#x22;value&#x22; &#x3a; &#x22;New status&#x22;,&#xd;&#xa;&#x2f;&#x2f;				&#x22;strict&#x22; &#x3a; false&#xd;&#xa;&#x2f;&#x2f;			&#x7d;&#xd;&#xa;&#x2f;&#x2f;		&#x5d;&#xd;&#xa;&#x2f;&#x2f;	&#x7d;&#xd;&#xa;&#x2f;&#x2f;&#x5d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;var jsonString&#x3b;&#xd;&#xa;&#x2f;&#x2f;&#xd;&#xa;&#x2f;&#x2f; Post initialize&#x3b; handle normal rows where current row is equal to the row id.&#xd;&#xa;&#x2f;&#x2f;&#xd;&#xa;writeToLog&#x28;&#x22;m&#x22;,current_ROW_ID&#x29;&#x3b;&#xd;&#xa;writeToLog&#x28;&#x22;m&#x22;,ROW_ID&#x29;&#x3b;&#xd;&#xa;if&#x28;String&#x28;current_ROW_ID&#x29; &#x3d;&#x3d; &#x28;String&#x28;ROW_ID&#x29;&#x29; &#x29; &#x7b;&#xd;&#xa;&#xd;&#xa;	&#x2f;&#x2f; Push additional cells into a row as they continue to come in for an existing row.&#xd;&#xa;	var cellDetail &#x3d; &#x7b;&#x7d;&#x3b;&#xd;&#xa;	cellDetail.columnId &#x3d; COLUMN_ID&#x3b;&#xd;&#xa;	cellDetail.value &#x3d; VALUE&#x3b;&#xd;&#xa;	rowDetail.cells.push&#x28;cellDetail&#x29;&#x3b;&#xd;&#xa;	writeToLog&#x28;&#x22;m&#x22;,&#x22;On same row&#x3a; &#x22; &#x2b; JSON.stringify&#x28;rowDetail&#x29;&#x29;&#x3b;&#xd;&#xa;	if&#x28;IS_LAST_ROW &#x3d;&#x3d; true&#x29; &#x7b;&#xd;&#xa;		pushRowDetailsIntoGlobalRowsObject&#x28;rowDetail&#x29;&#x3b;&#xd;&#xa;	&#x7d;&#xd;&#xa;&#x7d;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f;&#xd;&#xa;&#x2f;&#x2f; Initialize this static variable with the first row.&#xd;&#xa;&#x2f;&#x2f;&#xd;&#xa;if&#x28;String&#x28;current_ROW_ID&#x29; &#x3d;&#x3d; &#x28;String&#x28;&#x22;-1&#x22;&#x29;&#x29;&#x29; &#x7b;&#xd;&#xa;	current_ROW_ID &#x3d; ROW_ID&#x3b;&#xd;&#xa;&#xd;&#xa;	&#x2f;&#x2f; Prepare the rowDetail object to start receiving cell values on the first row ever.&#xd;&#xa;	rowDetail &#x3d; &#x7b;&#x7d;&#x3b;&#xd;&#xa;	rowDetail.toBottom &#x3d; true&#x3b;&#xd;&#xa;	rowDetail.cells &#x3d; &#x5b;&#x5d;&#x3b;&#xd;&#xa;&#xd;&#xa;	&#x2f;&#x2f; Push the cells that came in from that first row&#xd;&#xa;	var cellDetail &#x3d; &#x7b;&#x7d;&#x3b;&#xd;&#xa;	cellDetail.columnId &#x3d; COLUMN_ID&#x3b;&#xd;&#xa;	cellDetail.value &#x3d; VALUE&#x3b;&#xd;&#xa;	rowDetail.cells.push&#x28;cellDetail&#x29;&#x3b;&#xd;&#xa;	writeToLog&#x28;&#x22;m&#x22;,&#x22;Initialize&#x3a; &#x22; &#x2b; JSON.stringify&#x28;rowDetail&#x29;&#x29;&#x3b;  &#xd;&#xa;&#x7d;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f;&#xd;&#xa;&#x2f;&#x2f; New Row Case&#x3b; close up old one, start on new one.&#xd;&#xa;&#x2f;&#x2f;&#xd;&#xa;&#x2f;&#x2f; When they do not match, we must be starting a new row of cells, so push all existing cells into the row to&#xd;&#xa;&#x2f;&#x2f; prepare to deal with the next newer row.&#xd;&#xa;if&#x28;String&#x28;current_ROW_ID&#x29; &#x21;&#x3d; &#x28;String&#x28;ROW_ID&#x29;&#x29; &#x29; &#x7b;&#xd;&#xa;&#xd;&#xa;	&#x2f;&#x2f; since we are starting on a new row, set the current pointer to that new row id&#xd;&#xa;	current_ROW_ID &#x3d; ROW_ID&#x3b;&#xd;&#xa;	writeToLog&#x28;&#x22;m&#x22;,&#x22;rowDetail before pushing to rows&#x3a; &#x22; &#x2b; JSON.stringify&#x28;rowDetail&#x29;&#x29;&#x3b;&#xd;&#xa;	&#x2f;&#x2f;Alert&#x28;&#x22;rowDetail before pushing to rows&#x3a; &#x22; &#x2b; JSON.stringify&#x28;rowDetail&#x29;&#x29;&#x3b;&#xd;&#xa;	&#x2f;&#x2f; push the newly finished row onto the rows array&#xd;&#xa;	&#x2f;&#x2f;rows.push&#x28;rowDetail&#x29;&#x3b;&#xd;&#xa;	pushRowDetailsIntoGlobalRowsObject&#x28;rowDetail&#x29;&#x3b;&#xd;&#xa;	writeToLog&#x28;&#x22;m&#x22;,&#x22;after pushing rowDetail to rows&#x3a; &#x22; &#x2b; JSON.stringify&#x28;rows&#x29;&#x29;&#x3b;  &#xd;&#xa;&#xd;&#xa;	&#x2f;&#x2f; reset the rowDetail object after a row gets pushed onto the rows array&#xd;&#xa;	rowDetail &#x3d; &#x7b;&#x7d;&#x3b;&#xd;&#xa;	rowDetail.toBottom &#x3d; true&#x3b;&#xd;&#xa;	rowDetail.cells &#x3d; &#x5b;&#x5d;&#x3b;&#xd;&#xa;&#xd;&#xa;	&#x2f;&#x2f; Push the cells that came in on this new row&#xd;&#xa;	var cellDetail &#x3d; &#x7b;&#x7d;&#x3b;&#xd;&#xa;	cellDetail.columnId &#x3d; COLUMN_ID&#x3b;&#xd;&#xa;	cellDetail.value &#x3d; VALUE&#x3b;&#xd;&#xa;	rowDetail.cells.push&#x28;cellDetail&#x29;&#x3b;&#xd;&#xa;	writeToLog&#x28;&#x22;m&#x22;,&#x22;rowDetails after pushing cells from the new row&#x3a; &#x22; &#x2b; JSON.stringify&#x28;rowDetail&#x29;&#x29;&#x3b;  &#xd;&#xa;&#x7d;&#xd;&#xa;&#xd;&#xa;if&#x28;IS_LAST_ROW &#x3d;&#x3d; false&#x29; &#x7b;&#xd;&#xa;	trans_Status&#x3d;SKIP_TRANSFORMATION&#x3b;  &#x2f;&#x2f; consume the input row and don&#x27;t emit an output row&#xd;&#xa;&#x7d; else &#x7b;&#xd;&#xa;	jsonString &#x3d; JSON.stringify&#x28;rows&#x29;&#x3b;&#xd;&#xa;	trans_Status&#x3d;CONTINUE_TRANSFORMATION&#x3b; &#x2f;&#x2f; emit the output row in this case.&#xd;&#xa;&#x7d;</jsScript_script>
